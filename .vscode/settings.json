{
    "[dart]": {
        "editor.rulers": [
            120
        ]
    },
    "dart.flutterSdkPath": ".fvm/flutter_sdk",
    "dart.lineLength": 120,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "files.watcherExclude": {
        "**/.fvm": true
    },
    "search.exclude": {
        "**/.fvm": true
    }
}